{
  "name": "cac-panel-extension",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.5",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "bootstrap": "^4.6.0",
    "cross-env": "^7.0.3",
    "husky": "^4.3.8",
    "node-html-parser": "^3.1.0",
    "popper.js": "^1.16.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "startbootstrap-sb-admin-2": "^4.1.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:popup && npm run build:background",
    "build:popup": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:background": "tsc ./src/background.ts ./src/api.ts --outDir ./build --target es5 --module es2015 --moduleResolution node",
    "test": "react-scripts test",
    "prettier": "prettier --write --ignore-unknown .",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && git add -A ."
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "0.0.133"
  }
}
