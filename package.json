{
  "name": "cac-panel-extension",
  "version": "0.1.17",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@sentry/react": "^6.2.3",
    "@sentry/tracing": "^6.2.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.5",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "bootstrap": "^4.6.0",
    "bootstrap-switch-button-react": "^1.2.0",
    "cross-env": "^7.0.3",
    "node-html-parser": "^3.1.0",
    "popper.js": "^1.16.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "startbootstrap-sb-admin-2": "^4.1.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "cross-env NODE_ENV=development react-app-rewired build",
    "test": "react-app-rewired test --verbose --watchAll=false",
    "prettier": "prettier --write --ignore-unknown .",
    "eject": "react-app-rewired eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.test.[jt]s?(x)?"
    ],
    "rootDir": ".",
    "setupFiles": [
      "./tests/setupJest.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "0.0.133",
    "chrome-manifest-sync-webpack-plugin": "^1.0.1",
    "husky": "^6.0.0",
    "jest-fetch-mock": "^3.0.3"
  }
}
